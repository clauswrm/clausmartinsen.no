{
  "name": "clausmartinsen.no",
  "version": "1.0.0",
  "description": "Personal Website",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "start": "parcel src/index.html --port 3000 --open",
    "build": "parcel build src/index.html --no-source-maps && node ./postbuild.js",
    "watch": "parcel watch src/index.html",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format-check": "prettier --check src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clauswrm/clausmartinsen.no.git"
  },
  "author": "Claus Martinsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clauswrm/clausmartinsen.no/issues"
  },
  "homepage": "https://github.com/clauswrm/clausmartinsen.no#readme",
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-html-root-syntax-all-links": "0.0.1",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "parcel-plugin-sw-cache": "^0.3.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**",
    "excludeGlob": ["ga", "ga/**"]
  },
  "jest": {
    "coverageReporters": [
      "html"
    ]
  },
  "cache": {
    "clearDist": true
  }
}
